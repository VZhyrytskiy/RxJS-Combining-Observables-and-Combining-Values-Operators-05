{
  "name": "rxjs-typescript-starter",
  "scripts": {
    "project": "./node_modules/.bin/webpack-dev-server --config webpack.dev.js",
    "build": "./node_modules/.bin/webpack --config webpack.dev.js",
    "server": "json-server --watch db/db.json --port 3001",
    "start": "concurrently --kill-others \"npm run project\" \"npm run server\""
  },
  "dependencies": {
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@webpack-cli/init": "0.0.8",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "concurrently": "^4.1.0",
    "json-server": "^0.14.0",
    "ts-loader": "4.4.1",
    "typescript": "2.9.1",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.3",
    "webpack-dev-server": "3.1.4"
  }
}
